{
  "name": "expenses",
  "version": "2.0.0",
  "engines": {
    "node": "14.x"
  },
  "description": "App to manage food expenses along the month",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev:server": "npx ts-node-dev --transpile-only --inspect --ignore node_modules src/server.ts",
    "typeorm": "npx ts-node-dev ./node_modules/typeorm/cli.js",
    "start": "node dist/server.js",
    "prod:migrate": "npx typeorm migration:run"
  },
  "keywords": [
    "expenses",
    "food",
    "app"
  ],
  "optionalDependencies": {
    "fsevents": "2.3.1"
  },
  "author": "Ricardo Guizi",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-openapi-validator": "^3.16.1",
    "googleapis": "^74.2.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.25",
    "uuidv4": "^6.2.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
